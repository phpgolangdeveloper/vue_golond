     this.$axios.post(\"/vue/post_book\", {\n              name: toRaw(modelRef).name,\n              password: toRaw(modelRef).pasw,\n            }).then((res) => {\n              console.log(res);\n            }).catch((res) => {\n              console.log(res);\n            });\n          })\n          .catch(err => {\n            console.log('error', err);\n          });\n    };\n    return {\n      labelCol: {span: 4},\n      wrapperCol: {span: 14},\n      validate,\n      validateInfos,\n      resetFields,\n      modelRef,\n      onSubmit,\n    };\n  },\n});\n</script>\n\n"]}]}